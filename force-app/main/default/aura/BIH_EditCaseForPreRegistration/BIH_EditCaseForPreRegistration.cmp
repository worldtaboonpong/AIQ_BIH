                                            <lightning:button variant="brand" class="btn-100-width" iconName="utility:search" label="Verified"
                                        title="Verified" onclick="{! c.onSearchPDVS }" />
                                            <aura:set attribute="else">


                                                <aura:if isTrue="{! v.isNew }">
                                                    <lightning:button variant="destructive" class="btn-100-width" iconName="utility:search" label="Verify"
