                    <div class="slds-col slds-size_1-of-2">
                        <lightning:input name="created_by" readonly="true" label="Created By"
                            value="{!v.caseObj.CreatedBy.Name}" />
                        <lightning:input name="last_modified_by" readonly="true" label="Last Modified By"
                            value="{!v.caseObj.LastModifiedBy.Name}" />
                             <label class="slds-form-element__label">Is Vitallife Case?</label>
                             <ui:inputCheckbox aura:id="" label="" value="{!v.caseObj.Is_Vitallife_Case__c}"/>
                        <!-- <lightning:input name="created_date" readonly="true" label="Created Date" value="{!v.caseObj.CreatedDate}" /> -->
                    </div>
                    <div class="slds-col slds-size_1-of-2">
                        <label class="slds-form-element__label">Created Date</label>
                        <div class="slds-form-element__control slds-grow"
                            style="height: 32px;display: flex;border-bottom: 1px solid rgb(221, 219, 218);">

                            <lightning:formattedDateTime value="{!v.caseObj.CreatedDate}" year="numeric" month="short"
                                day="numeric" hour="2-digit" minute="2-digit" hour12="false" />
                        </div>

                        <label class="slds-form-element__label">Last Modified Date</label>
                        <div class="slds-form-element__control slds-grow"
                            style="height: 32px;display: flex;border-bottom: 1px solid rgb(221, 219, 218);">
                            <lightning:formattedDateTime value="{!v.caseObj.LastModifiedDate}" year="numeric"
                                month="short" day="numeric" hour="2-digit" minute="2-digit" hour12="false" />
                        </div>
                        <!--<lightning:input name="last_modified_date" readonly="true" label="Last Modified Date" value="{!v.caseObj.LastModifiedDate}" />-->
                             <label class="slds-form-element__label">Is TeleMed Case?</label>
                             <ui:inputCheckbox aura:id="" label="" value="{!v.caseObj.Is_TeleMed_Case__c}"/>              
                             <!--<lightning:input name="Is_TeleMed_Case" type="checkbox" value="{!v.caseObj.Is_TeleMed_Case__c}" />-->
                    </div>
                </div>
            </div>
            <!-- <c:BIH_GeneralWebInformation isView="false" caseObj="{!v.caseObj}" />
            <aura:renderIf isTrue="{!v.isContactCenter}" >
            <h3 class="slds-section__title slds-theme_shade last-section">
                <span class="slds-truncate slds-p-horizontal_small" title="Section Title">Campaign</span>
            </h3>
            <div aura:id="CaseCampaignSession">
            <c:BIH_CaseCampaign aura:id="BIH_CaseCampaign" recordId="{!v.recordId}"/>
            </div> -->
            
        <!-- </aura:renderIf> -->
        <!-- <aura:if isTrue="{!v.isContactCenter}"> -->
        <!-- <div class="container-btn-group"> -->
            <!-- button change onsave to onevent -->
        <!-- </div> -->
        <!-- <aura:set attribute="else"> -->
            <div class="container-btn-group">
                <lightning:button class="slds-button slds-button--neutral uiButton--neutral uiButton forceActionButton"
                    label="Cancel" title="Cancel" onclick="{! c.onCancel }" />
                <lightning:button class="slds-button slds-button--neutral uiButton--brand uiButton forceActionButton"
                    label="Save" title="Save" onclick="{! c.onSave }" /> 
            </div>
        <!-- </aura:set> -->
        <!-- </aura:if> -->
        </div>
    </div>

    <aura:if isTrue="{!v.ShowModule}">
        <div style="max-width: 50rem;">
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
                aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container">
                    <header class="slds-modal__header">
                        <a class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close"
                            onclick="{! c.hideModal }">
                            <lightning:icon iconName="utility:close" class="slds-icon slds-icon_small" size="small"
                                alternativeText="close" />
                        </a>
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Search Patient
                        </h2>
                    </header>
                    <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                        <c:BIH_VerifyPatient isFirstTime="true" hnNumber="{!v.caseObj.Hospital_Number_HN__c}"
                            dateOfBirth="{!v.caseObj.Date_of_Birth__c}" fullName="{!v.caseObj.Full_Name__c}"
                            gender="{!v.caseObj.Gender__c}" recordId="{!v.recordId}" />
                    </div>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </div>
    </aura:if>
</aura:component>