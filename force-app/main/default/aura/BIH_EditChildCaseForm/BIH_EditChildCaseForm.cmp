                                <option value="" selected="">--None--</option>
                                <aura:iteration items="{!v.PickList.To__c}" var="option">
                                        <option value="{!option.value}" selected="">{!option.label}</option>
                                </aura:iteration>
                            </lightning:select>

                            <lightning:input aura:id="to_other-hotel" name="to_other-hotel" label="To Other Hotel" value="{!v.caseObj.To_Other_Hotel__c}" />
