                    <div class="slds-grid slds-gutters">
                        <div class="slds-col slds-size_1-of-2">
                                <lightning:dualListbox name="multipleOptions"  
                                                        label= "Airport Rep's Name" 
                                                        sourceLabel="Available" 
                                                        selectedLabel="Selected" 
                                                        options="{!v.PickList.Airport_Rep_Name__c}" 
                                                        value="{!v.defaultRepOptions}" 
                                                        onchange="{! c.handleChangeAirportRep }"/>
                            </div>
                        <div class="slds-col slds-size_1-of-2">
                                <lightning:input type="number" name="other_service_charge" label="Other Service Charge" value="{!v.caseObj.Other_Service_Charge__c}" />

                        </div>
                    </div>
