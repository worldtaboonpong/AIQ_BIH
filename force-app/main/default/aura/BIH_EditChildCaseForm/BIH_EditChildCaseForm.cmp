                            <option value="" selected="">--None--</option>    
                            <aura:iteration items="{!v.PickList.Case_arr_dep__c}" var="option">
                                <option value="{!option.value}"  selected="">{!option.label}</option>
                                </aura:iteration>
                            </lightning:select>

                            <lightning:input type="date" name="arrivaldate" label="Arrival Date" value="{!v.caseObj.Arrival_Date__c}"/>

