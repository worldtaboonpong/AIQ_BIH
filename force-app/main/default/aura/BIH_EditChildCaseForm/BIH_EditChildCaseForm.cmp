                            <lightning:select aura:id="method_of_payment" name="method_of_payment" label="Method Of Payment" onchange="{! c.onChangePaymentMethod }" value="{!v.caseObj.Method_Of_Payment__c}">
                                <option value="" selected="">--None--</option>
                                <aura:iteration items="{!v.PickList.Method_Of_Payment__c}" var="option">
                                        <option value="{!option.value}" selected="">{!option.label}</option>
                                </aura:iteration>
                            </lightning:select>

                            <lightning:select aura:id="segment" name="segment" label="Segment" value="{!v.caseObj.Segment__c}">
                                    <option value="" selected="">--None--</option>    
                            <aura:iteration items="{!v.SegmentList}" var="option">
                                    <option value="{!option.value}" selected="">{!option.label}</option>
                                </aura:iteration>
                            </lightning:select>
                            <lightning:input name="cost_center" label="Cost Center" value="{!v.caseObj.Cost_Center__c}" />
                        </div>
                
                    </div>
                </div>
                <h3 class="slds-section__title slds-theme_shade">
                    <span class="slds-truncate slds-p-horizontal_small" title="Section Title">Airport Service Info</span>
                </h3>

                
