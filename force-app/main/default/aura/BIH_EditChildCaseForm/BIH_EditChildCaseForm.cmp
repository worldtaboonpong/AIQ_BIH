    <div>
       <!-- <aura:if isTrue="{! v.loading }">
            <lightning:spinner alternativeText="Loading" />
        </aura:if>-->
        <div class="slds-section slds-is-open">

            <h3 class="slds-section__title slds-theme_shade">
                <span class="slds-truncate slds-p-horizontal_small" title="Section Title">Contact Information</span>
            </h3>
            <div aria-hidden="false" class="slds-section__content">
                <div class="slds-grid slds-gutters">
                    <div class="slds-col slds-size_1-of-2">
                        <c:BIH_CustomLookup objectAPIName="account" fieldAPIName="Name" IconName="standard:account"
                            selectedRecord="{!v.senderNameValue}" showField="Email__c" label="Sender Name" />
                        <lightning:input aura:id="sender_email" name="sender_email" required="{!v.isRequiredSenderEmail }" label="Sender Email"
                            value="{!v.caseObj.SuppliedEmail}" />
                        <aura:if isTrue="{! (!v.loading )&amp;&amp; (v.isAirportTransferRecordType )}">
                        <ui:inputCheckbox name="walk_in" label="Walk-in" value="{!v.caseObj.Walk_in__c}" change="{!c.onClickWalkIn}"/>
                        </aura:if>
                    </div>
                    <div class="slds-col slds-size_1-of-2">
                        <div class="slds-grid slds-gutters">
                            <div class="slds-col slds-size_10-of-12">
                                <lightning:input name="patient_hn" label="Patient HN"
                                    value="{!v.caseObj.Hospital_Number_HN__c}" />
                            </div>
                            <div class="slds-col slds-size_2-of-12">
                                <div style="height:100%;display: flex;align-items: flex-end;justify-content: flex-end;">
                                    <!-- <lightning:button variant="brand" class="btn-100-width" iconName="utility:search" label="Verify"
                                        title="Verify" onclick="{! c.showModal }" /> -->
                                        <aura:if isTrue="{! v.caseObj.Verified_Patient__c }">
                                        
                                            <lightning:button variant="brand" class="btn-100-width" iconName="utility:search" label="Verified"
                                        title="Verified" onclick="{! c.onSearchPDVS }" />
                                            <aura:set attribute="else">


                                                <aura:if isTrue="{! v.isNew }">
                                                    <lightning:button variant="destructive" class="btn-100-width" iconName="utility:search" label="Verify"
                                        title="Verify" onclick="{! c.onSearchPDVS }"  />

                                        <aura:set attribute="else">
                                            <lightning:button variant="destructive" class="btn-100-width" iconName="utility:search" label="Verify"
                                        title="Verify" onclick="{! c.showModal }"  />
                                        </aura:set>

                                                    </aura:if>
                                                



                                        
                                            </aura:set>
                                        
                                        </aura:if>
                                </div>
                            </div>
                        </div>

                        <c:BIH_CustomLookup objectAPIName="account" fieldAPIName="Name" IconName="standard:account"
                            selectedRecord="{!v.patientNameValue}" label="Patient Name" />

                        <lightning:input aura:id="fullname" name="fullname" label="Fullname"
                        value="{!v.caseObj.Full_Name__c}" />
                    </div>

                </div>
            </div>
<!-- ####### Clinic Information ####### -->
