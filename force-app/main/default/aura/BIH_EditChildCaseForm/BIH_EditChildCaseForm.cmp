                                <aura:iteration items="{!v.PickList.Number_Of_Relative__c}" var="option">
                                    <option value="{!option.value}" selected="">{!option.label}</option>
                                </aura:iteration>
                            </lightning:select>

                            <lightning:select aura:id="to" name="to" label="To"
