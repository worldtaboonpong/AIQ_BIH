                                <aura:iteration items="{!v.PickList.Request_By__c}" var="option">
                                    <option value="{!option.value}" selected="" >{!option.label}</option>
                                </aura:iteration>
                            </lightning:select>
                            <lightning:input aura:id="other_request_by" name="other_request_by" label="Other Request By" value="{!v.caseObj.Other_Request_By__c}" />
