                                <aura:iteration items="{!v.PickList.Number_Of_Patient__c}" var="option">
                                    <option value="{!option.value}"  selected="">{!option.label}</option>
                                </aura:iteration>
                            </lightning:select>
                        
                            <lightning:select aura:id="from" name="From" label="From" value="{!v.caseObj.From__c}">
                                    <option value="" selected="">--None--</option>    
