                            <lightning:select aura:id="registration" name="registration" label="Registration" value="{!v.caseObj.Registration__c}">
                                    <option value="" selected="">--None--</option>   
                            <aura:iteration items="{!v.PickList.Registration__c}" var="option">
                                    <option value="{!option.value}" selected="">{!option.label}</option>
                                </aura:iteration>
                            </lightning:select>

                            <lightning:select aura:id="hotel" name="hotel" label="Hotel (Airport)" value="{!v.caseObj.Hotel_Airport__c}">
                                    <option value="" selected="">--None--</option>    
                            <aura:iteration items="{!v.PickList.Hotel_Airport__c}" var="option">
                                    <option value="{!option.value}"  selected="">{!option.label}</option>
                                </aura:iteration>
                            </lightning:select>
                        </div>
                        <div class="slds-col slds-size_1-of-2">
                            <lightning:select aura:id="appointment" name="appointment" label="Appointment" value="{!v.caseObj.Airport_Appointment__c}">
                                    <option value="" selected="">--None--</option>  
                            <aura:iteration items="{!v.PickList.Airport_Appointment__c}" var="option">
                                    <option value="{!option.value}" selected="">{!option.label}</option>
                                </aura:iteration>
                            </lightning:select>

                            <lightning:select aura:id="ro" name="ro" label="RO" value="{!v.caseObj.RO__c}">
                                    <option value="" selected="">--None--</option>    
                                <aura:iteration items="{!v.PickList.RO__c}" var="option">
                                        <option value="{!option.value}" selected="">{!option.label}</option>
                                </aura:iteration>
                            </lightning:select>

                        </div>
                    </div>
                </div>
                <div aria-hidden="false" class="slds-section__content">
                    <div class="slds-grid slds-gutters">
                        <div class="slds-col slds-size_1-of-1">
                            <lightning:textarea aura:id="note" name="note" label="Note" value="{!v.caseObj.Note__c}" />
                        </div>
                    </div>
                </div>         
            </aura:if>
        <!-- ####### Airport transfer section ####### -->
            
